      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
    </tr>
