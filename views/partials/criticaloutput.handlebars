<!--#region Property Value -->
<table class="table table-hover table-sm">
  <thead>
    <tr style="position: sticky; top:0">
      <th scope="col" style="min-width: 15rem;"></th>
      <th scope="col" style="min-width: 7rem;">At Purchase</th>
      <th scope="col" style="min-width: 7rem;">Year 1</th>
      <th scope="col" style="min-width: 7rem;">Year 2</th>
      <th scope="col" style="min-width: 7rem;">Year 3</th>
      <th scope="col" style="min-width: 7rem;">Year 4</th>
      <th scope="col" style="min-width: 7rem;">Year 5</th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Property Value</td>
      {{#each propertyReports.criticalOutput.propertyValue as |propertyValue|}}
      <td>{{formatCurrency propertyValue}}</td>
      {{/each}}
    </tr>
  </tbody>
</table>
<!--#endregion-->

<!--#region Income and Expenses -->
<table class="table table-hover table-sm">
  <thead>
    <tr>
      <th scope="col" style="min-width: 15rem;">Income and Expenses</th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>GSI</td>
      {{#each propertyReports.criticalOutput.GSI as |GSI|}}
      <td>{{formatCurrency GSI}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Loss due to Vacancies</td>
      {{#each propertyReports.criticalOutput.vacancy as |vacancy|}}
      <td>{{formatCurrency vacancy}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Gross Operating Income</td>
      {{#each propertyReports.criticalOutput.GOI as |GOI|}}
      <td>{{formatCurrency GOI}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Loss due to Expenses</td>
      {{#each propertyReports.criticalOutput.totalExpense as |totalExpense|}}
      <td>{{formatCurrency totalExpense}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Net Operating Income (NOI)</td>
      {{#each propertyReports.criticalOutput.NOI as |NOI|}}
      <td>{{formatCurrency NOI}}</td>
      {{/each}}
    </tr>
  </tbody>
</table>
<!--#endregion-->

<!--#region Financial Ratios -->
<table class="table table-hover table-sm">
  <thead>
    <tr>
      <th scope="col" style="min-width: 15rem;">Financial Ratios</th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>Gross Multiplier</td>
      <td>{{propertyReports.criticalOutput.grossMultiplier}}</td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    <tr>
      <td>Cap Rate</td>
      <td>{{propertyReports.criticalOutput.capRate}}</td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    <tr>
      <td>Internal Rate of Return</td>
      <td>{{propertyReports.criticalOutput.irr}}</td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    <tr>
      <td>Yield</td>
      <td>{{propertyReports.criticalOutput.yield}}</td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
      <td></td>
    </tr>
    <tr>
      <td>Debt Coverage Ratio</td>
      <td></td>
      {{#each propertyReports.criticalOutput.debtCoverage as |debtCoverage|}}
      <td>{{debtCoverage}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Loan to Value (LTV) Ratio</td>
      {{#each propertyReports.criticalOutput.ltv as |ltv|}}
      <td>{{limit ltv 5}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Ownership Percentage</td>
      <td></td>
      {{#each propertyReports.criticalOutput.ownPercent as |ownPercent|}}
      <td>{{ownPercent}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Cash on Cash Return (BT)</td>
      <td></td>
      {{#each propertyReports.criticalOutput.cashOnCashBeforeTax as |cashOnCashBeforeTax|}}
      <td>{{cashOnCashBeforeTax}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>Cash on Cash Return (AT)</td>
      <td></td>
      {{#each propertyReports.criticalOutput.cashOnCashAfterTax as |cashOnCashAfterTax|}}
      <td>{{cashOnCashAfterTax}}</td>
      {{/each}}
    </tr>
  </tbody>
</table>
<!--#endregion-->

<!--#region Depreciation -->
