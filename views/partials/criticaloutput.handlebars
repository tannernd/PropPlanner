<!--#endregion-->

<!--#region Rate of Return -->
<table class="table table-hover table-sm">
  <thead>
    <tr>
      <th scope="col" style="min-width: 15rem;">Rate of Return (RoR)</th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
      <th scope="col" style="min-width: 7rem;"></th>
    </tr>
  </thead>
  <tbody>
    <tr>
      <td>RoR on Loan Reduction</td>
      <td></td>
      {{#each propertyReports.criticalOutput.rorLoanReduction as |rorLoanReduction|}}
      <td>{{suffix (limit rorLoanReduction 5) '%'}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>RoR on Appreciation</td>
      <td></td>
      {{#each propertyReports.criticalOutput.rorAppreciation as |rorAppreciation|}}
      <td>{{suffix (limit rorAppreciation 5) '%'}}</td>
      {{/each}}
    </tr>
    <tr>
      <td>RoR on BTCF</td>
      <td></td>
      {{#each propertyReports.criticalOutput.rorBeforeTaxCF as |rorBeforeTaxCF|}}
      <td>{{suffix (limit rorBeforeTaxCF 5) '%'}}</td>
      {{/each}}
    </tr>
  </tbody>
</table>
<!--#endregion-->